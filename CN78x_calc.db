record(calc,"XF:11BM{LA:Omega}_T_RBV") {
    field(EGU,"deg C")
    field(CALC,"A/10")
    field(INPA,"XF:11BM{LA:Omega}T_RBV CPP")
    field(SCAN, "Passive")
    field(PINI, 1)
    field(PREC, 1)
}
record(calc,"XF:11BM{LA:Omega}_SP_RBV") {
    field(EGU,"deg C")
    field(CALC,"A/10")
    field(INPA,"XF:11BM{LA:Omega}SP_RBV CPP")
    field(SCAN, "Passive")
    field(PINI, 1)
    field(PREC, 1)
}

record(ai, "XF:11BM{LA:Omega}_SP"){
    field(EGU, "deg C")
    field(SCAN, "Passive")
    field(PREC, "1")
    field(PINI, 0)
}
record(calcout,"XF:11BM{LA:Omega}SP_out") { 
    field(EGU,"deg C")
    field(CALC,"A*10")
    field(INPA,"XF:11BM{LA:Omega}_SP CPP")
    field(SCAN, "Passive")
    field(PREC, 1)
    field(PINI, 0)
    field(OOPT, "When Non-zero")
    field(OUT, "XF:11BM{LA:Omega}SP PP ")
}

